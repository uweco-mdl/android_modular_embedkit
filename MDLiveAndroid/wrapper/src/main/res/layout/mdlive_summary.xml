<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        style="@style/ContainerStyle"
        android:background="@color/mdliveBottomBlueColor">

        <ScrollView
            style="@style/ScrollViewHolderStyle"
            android:background="@color/mdliveBottomBlueColor">

            <LinearLayout
                style="@style/ContainerStyle"
                android:background="@color/white">

                <RelativeLayout
                    style="@style/wagHeaderLinearLayoutStyle"
                    android:orientation="horizontal">


                    <TextView
                        android:id="@+id/homeImg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:text="@string/your_consultation"
                        android:textColor="@color/white"
                        android:textSize="20sp" />
                </RelativeLayout>

                <LinearLayout
                    style="@style/common_style_W_Match_H_Wrap_O_Vertical"
                    android:layout_marginLeft="5dp"
                    android:background="@color/myWindowBackground"
                    android:padding="5dp">


                    <TextView
                        android:id="@+id/textView9"
                        style="@style/common_style_W_Match_M_Match"
                        android:layout_marginTop="15dp"
                        android:text="@string/summary_thankyou_txt"
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textView10"
                        style="@style/common_style_W_Match_M_Match"
                        android:layout_marginTop="25dp"
                        android:text="@string/summary"
                        android:textColor="@color/darkblack"
                        android:textSize="16sp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtDoctorName"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/test"
                                android:textColor="@color/black"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/txtTotalTime"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="@color/black"
                                android:textSize="16sp"
                                android:visibility="gone" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtPaymentSummary"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/dollar_Amount49"
                                android:textColor="@color/black"
                                android:textSize="25sp" />

                        </LinearLayout>
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/textView15"
                        style="@style/common_style_W_Match_M_Match"
                        android:layout_marginTop="20dp"
                        android:text="@string/rate_consulatation"
                        android:textColor="@color/darkblack"
                        android:textSize="16sp" />

                    <RatingBar
                        android:id="@+id/ratingBar"
                        style="@style/RatingBarTheme"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:clickable="true"
                        android:numStars="5"
                        android:stepSize="1" />

                    <TextView
                        android:id="@+id/textView16"
                        style="@style/common_style_W_Match_M_Match"
                        android:layout_marginTop="15dp"
                        android:text="@string/next_steps_txt"
                        android:textColor="@color/darkblack"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/NextStepsContentTv"
                        style="@style/common_style_W_Match_M_Match"
                        android:layout_marginTop="15dp"
                        android:text=""
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                    <Button
                        android:id="@+id/DoneRatingBtn"
                        style="@style/ContinueButtonStyle"
                        android:layout_margin="10dp"
                        android:layout_marginTop="18dp"
                        android:text="@string/done"
                        android:onClick="sendRating"
                        android:textAllCaps="false"
                        android:textSize="16sp" />
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white">

                    <include
                        style="@style/Footer_Style"
                        layout="@layout/mdlive_footer"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="0dp"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"

                        android:layout_gravity="bottom" />

                </RelativeLayout>
            </LinearLayout>
        </ScrollView>

    </LinearLayout>

    <include layout="@layout/custom_progress_dialog" />
</merge>