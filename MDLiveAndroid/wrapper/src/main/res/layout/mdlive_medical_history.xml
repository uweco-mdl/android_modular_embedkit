<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content">

    <LinearLayout
        style="@style/ContainerStyle"
        android:background="@color/choose_pharmacy_background">

        <ScrollView
            android:id="@+id/ContainerScrollView"
            style="@style/ScrollViewStyle"
            android:layout_marginBottom="@dimen/medical_history_margin_twelve"
            android:background="@color/choose_pharmacy_background"
            android:fillViewport="false"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/medical_history_common_margin"
                android:layout_marginTop="@dimen/medical_history_common_margin"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/UpdateInfoWindow"
                    style="@style/common_style_W_Match_H_Wrap_O_Vertical"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/updateInfoText"
                        style="@style/BaseTextViewStyle.MedicalHistory"
                        android:text="Last Updated: May 1, 2015" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/PediatricAgeCheck1"
                    android:visibility="gone">
                    <LinearLayout
                        style="@style/BaseLayoutStyle_MedicalHistory"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/PediatricChecks"
                            style="@style/BaseTextViewStyle.MyhealthContentStyle"
                            android:text="@string/age_check1" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioGroup
                                android:id="@+id/pediatricAgeGroup1"
                                style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/perdiatric1YesButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:text="Yes" />

                                <RadioButton
                                    android:id="@+id/perdiatric1NoButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:layout_marginLeft="@dimen/medical_history_margin_ten"
                                    android:text="No" />

                            </RadioGroup>
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        style="@style/ViewStyle.MedicalHistory" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/PediatricAgeCheck2"
                    android:visibility="gone">

                    <LinearLayout
                        style="@style/BaseLayoutStyle_MedicalHistory"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/PediatricCheck"
                            style="@style/BaseTextViewStyle.MyhealthContentStyle"
                            android:text="@string/age_check2" />


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioGroup
                                android:id="@+id/pediatricAgeGroup2"
                                style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/perdiatric2YesButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:text="Yes" />

                                <RadioButton
                                    android:id="@+id/perdiatric2NoButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:layout_marginLeft="@dimen/medical_history_margin_ten"
                                    android:text="No" />

                            </RadioGroup>
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        style="@style/ViewStyle.MedicalHistory"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/MyHealthConditionChoiceLl">

                    <LinearLayout
                        style="@style/BaseLayoutStyle_MedicalHistory"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/BaseTextViewStyle.MyhealthContentStyle"
                            android:text="@string/my_health_preexisting" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioGroup
                                android:id="@+id/conditionsGroup"
                                style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/conditionYesButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:text="Yes"
                                    android:checked="false" />

                                <RadioButton
                                    android:id="@+id/conditionNoButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:layout_marginLeft="@dimen/medical_history_margin_ten"
                                    android:text="No" />

                            </RadioGroup>
                        </LinearLayout>

                    </LinearLayout>

                    <View
                        style="@style/ViewStyle.MedicalHistory" />

                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/MyHealthConditionsLl"
                    style="@style/MedicalHistory_BasicLayout"
                    android:onClick="MyHealthConditionsLlOnClick"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/linearLayout"
                        style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="21dp"
                                android:layout_height="18dp"
                                android:src="@drawable/pre_existing_condition"
                                android:id="@+id/pre_existing_conditions_icon"
                                android:layout_alignBottom="@+id/textView4"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_marginRight="@dimen/medical_history_dimen_eight"/>

                            <TextView
                                style="@style/BaseTextViewStyle.MyhealthListLayoutSubTextStyle"
                                android:layout_width="match_parent"
                                android:text="Pre-Existing Conditions"
                                android:textSize="@dimen/medical_history_text_size"
                                android:layout_toRightOf="@+id/pre_existing_conditions_icon"
                                android:id="@+id/textView4"/>

                        </RelativeLayout>

                        <TextView
                            android:id="@+id/MyHealthConditionsNameTv"
                            style="@style/MyhealthListLayoutSubSubTextStyle"
                            android:layout_width="match_parent"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="No conditions reported" />

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/editConditionsIcon"
                        style="@style/common_style_W_Wrap_H_Wrap"
                        android:paddingRight="@dimen/medical_history_arrow_padding"
                        android:src="@drawable/right_arrow_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/MyHealthMedicationsLl">

                    <LinearLayout
                        style="@style/BaseLayoutStyle_MedicalHistory"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/BaseTextViewStyle.MyhealthContentStyle"
                            android:text="@string/my_health_medications" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioGroup
                                android:id="@+id/medicationsGroup"
                                style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/medicationsYesButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:checked="false"
                                    android:text="Yes" />

                                <RadioButton
                                    android:id="@+id/medicationsNoButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:layout_marginLeft="@dimen/medical_history_margin_ten"
                                    android:text="No" />

                            </RadioGroup>
                        </LinearLayout>

                    </LinearLayout>

                    <View
                        style="@style/ViewStyle.MedicalHistory" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/MedicationsLl"
                    style="@style/MedicalHistory_BasicLayout"
                    android:onClick="MedicationsLlOnClick"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/linearLayout1"
                        style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="16dp"
                                android:layout_height="21dp"
                                android:src="@drawable/medications"
                                android:id="@+id/medication_icon"
                                android:layout_alignTop="@+id/textView5"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_marginRight="@dimen/medical_history_margin_twelve"/>

                            <TextView
                                style="@style/BaseTextViewStyle.MyhealthListLayoutSubTextStyle"
                                android:layout_width="match_parent"
                                android:text="Medications"
                                android:textSize="@dimen/medical_history_text_size"
                                android:id="@+id/textView5"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@+id/medication_icon"
                                android:layout_toEndOf="@+id/medication_icon" />

                        </RelativeLayout>

                        <TextView
                            android:id="@+id/MedicationsNameTv"
                            style="@style/MyhealthListLayoutSubSubTextStyle"
                            android:layout_width="match_parent"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="No conditions reported" />

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/editMedicationsIcon"
                        style="@style/common_style_W_Wrap_H_Wrap"
                        android:paddingRight="@dimen/medical_history_arrow_padding"
                        android:src="@drawable/right_arrow_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/MyHealthAllergiesLl">

                    <LinearLayout
                        style="@style/BaseLayoutStyle_MedicalHistory"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:id="@+id/Primary_txt"
                            style="@style/BaseTextViewStyle.MyhealthContentStyle"
                            android:text="@string/my_health_allergies" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioGroup
                                android:id="@+id/allergiesGroup"
                                style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/allergiesYesButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:text="Yes" />

                                <RadioButton
                                    android:id="@+id/allergiesNoButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:layout_marginLeft="@dimen/medical_history_margin_ten"
                                    android:text="No" />

                            </RadioGroup>
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        style="@style/ViewStyle.MedicalHistory" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/AllergiesLl"
                    style="@style/MedicalHistory_BasicLayout"
                    android:onClick="AllergiesLlOnClick"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/linearLayout3"
                        style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <ImageView
                                android:layout_width="21dp"
                                android:layout_height="19dp"
                                android:src="@drawable/my_allergies"
                                android:id="@+id/allergies_icon"
                                android:layout_alignTop="@+id/textView6"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_marginRight="@dimen/medical_history_dimen_eight"/>

                            <TextView
                                style="@style/BaseTextViewStyle.MyhealthListLayoutSubTextStyle"
                                android:layout_width="match_parent"
                                android:text="Allergies"
                                android:textSize="@dimen/medical_history_text_size"
                                android:id="@+id/textView6"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@+id/allergies_icon"
                                android:layout_toEndOf="@+id/allergies_icon"/>
                        </RelativeLayout>

                        <TextView
                            android:id="@+id/AlergiesNameTv"
                            style="@style/MyhealthListLayoutSubSubTextStyle"
                            android:layout_width="match_parent"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="No conditions reported" />

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/editAllergiesIcon"
                        style="@style/common_style_W_Wrap_H_Wrap"
                        android:paddingRight="@dimen/medical_history_arrow_padding"
                        android:src="@drawable/right_arrow_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/MyPrimaryCareLl">
                    <LinearLayout
                        style="@style/BaseLayoutStyle_MedicalHistory"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/Primary_Care_Provider_Txt"
                            style="@style/BaseTextViewStyle.MyhealthContentStyle"
                            android:text="@string/primary_txt" />


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioGroup
                                android:id="@+id/primaryCareGroup"
                                style="@style/BaseTextViewStyle.MyhealthChoiceBtnStyle"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/primaryCareYesButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:text="Yes" />

                                <RadioButton
                                    android:id="@+id/primaryCareNoButton"
                                    style="@style/YesNoRadioButtonStyle"
                                    android:layout_marginLeft="@dimen/medical_history_margin_ten"
                                    android:text="No" />

                            </RadioGroup>
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        style="@style/ViewStyle.MedicalHistory" />
                </LinearLayout>

            </LinearLayout>

        </ScrollView>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:layout_alignParentBottom="true">
            <Button
                android:id="@+id/SavContinueBtn"
                style="@style/btnContinueStyle"
                android:onClick="SavContinueBtnOnClick"
                android:text="@string/continue_text"
                />
        </RelativeLayout>

    </LinearLayout>

    <include layout="@layout/custom_progress_dialog" />
</merge>