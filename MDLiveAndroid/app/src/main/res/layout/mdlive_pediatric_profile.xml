<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ContainerStyle">
    />

    <com.mdlive.unifiedmiddleware.commonclasses.customUi.CustomTv
        style = "@style/HeaderTextViewStyle"
        android:text="Pediatric Profile"
        >
    </com.mdlive.unifiedmiddleware.commonclasses.customUi.CustomTv>
    <View
        style="@style/ViewStyle"
        />
    <ListView
        android:animationCache="false"
        android:scrollingCache="false"
        android:smoothScrollbar="true"
        style="@style/PediatricListviewViewStyle"
        android:id="@+id/pediatricList" />

    <Button
        android:id="@+id/SavContinueBtn"
        style="@style/ContinueButtonStyle"
        android:text="Save"
        android:textAllCaps="false" />


</LinearLayout>