<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mdlive.embedkit"
    android:hardwareAccelerated="true">


    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>


    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.flash"
        android:required="false" />

    <application
        android:largeHeap="true"
      >

        <activity
            android:name=".uilayer.login.SSOActivity" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"
            android:noHistory="true">
        </activity>

        <!--Provider Details-->

        <activity
            android:name=".uilayer.sav.MDLiveGetStarted"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="adjustResize|stateAlwaysHidden"
           ></activity>

        <activity
            android:name=".uilayer.sav.MDLiveFamilymember"
            android:screenOrientation="portrait" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="stateHidden"></activity>

        <activity
            android:name="com.mdlive.embedkit.uilayer.sav.MDLiveChooseProvider"
            android:screenOrientation="portrait" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="stateHidden"></activity>

        <activity
            android:name="com.mdlive.embedkit.uilayer.sav.MDLiveSearchProvider"
            android:screenOrientation="portrait" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="stateHidden"></activity>


        <activity
            android:name="com.mdlive.embedkit.uilayer.sav.MDLiveProviderDetails"
            android:screenOrientation="portrait" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="stateHidden"></activity>

        <activity
            android:name="com.mdlive.embedkit.uilayer.sav.MDLiveReasonForVisit"
            android:screenOrientation="portrait" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name="com.mdlive.embedkit.uilayer.sav.MDLiveLocation"
            android:screenOrientation="portrait" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="stateHidden"></activity>


        <!-- Pharmacy Activities-->
        <activity
            android:name="com.mdlive.embedkit.uilayer.pharmacy.MDLivePharmacy" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.mdlive.embedkit.uilayer.pharmacy.MDLivePharmacyChange" android:theme="@style/MDLIVEactivityTheme"
            android:windowSoftInputMode="stateHidden"
            android:screenOrientation="portrait"></activity>

       <!-- <activity
            android:name="com.mdlive.embedkit.uilayer.pharmacy.MDLivePharmacyDetails"
            android:screenOrientation="portrait"></activity>-->

        <activity
            android:name="com.mdlive.embedkit.uilayer.pharmacy.MDLivePharmacyResult" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>


        <activity
            android:name=".uilayer.myhealth.MDLiveMedicalHistory" android:theme="@style/MDLIVEactivityTheme"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".uilayer.myhealth.imageadapter.MDLiveImageGalleryView" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".uilayer.myhealth.MDLiveAddAllergies" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".uilayer.myhealth.MDLiveAddConditions" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".uilayer.myhealth.MDLiveAddMedications" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>


        <!-- Payment Activity -->

        <activity
            android:name="com.mdlive.embedkit.uilayer.payment.MDLivePayment" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="com.mdlive.embedkit.uilayer.login.MDLiveSummary" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>



        <activity
            android:name=".MDLiveVsee" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name="com.mdlive.embedkit.uilayer.WaitingRoom.MDLiveWaitingRoom"
            android:label="@string/app_name" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>



        <activity
            android:name=".uilayer.pediatric.MDLivePediatric"
            android:windowSoftInputMode="stateHidden" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".uilayer.PendingVisits.MDLivePendingVisits" android:theme="@style/MDLIVEactivityTheme"
            android:screenOrientation="portrait"
             >
        </activity>



    </application>

</manifest>
